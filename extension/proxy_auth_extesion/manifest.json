{
    "manifest_version": 3,
    "name": "Proxy Auto Auth",
    "version": "1.0",
    "description": "Automatically authenticate to proxy server",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "proxy",
        "storage",
        "webRequest",
        "webRequestAuthProvider"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}